    "scripts": {
        "start": "npm-run-all --parallel watch:twig watch:bundle watch:iconfont",
        "build": "npm-run-all clean build:iconfont compile:bundle build:twig css-paths",
        "clean": "rimraf dist/*",
        "watch:twig": "watch 'yarn run compile:twig' ./src/views --interval=0.1",
        "watch:iconfont": "node ./scripts/iconfont.js -w",
        "watch:bundle": "webpack-dev-server --env dev",
        "compile:twig": "NODE_ENV=development node scripts/compile-twig.js",
        "build:twig": "NODE_ENV=production node scripts/compile-twig.js",
        "build:iconfont": "node ./scripts/iconfont.js",
        "compile:bundle": "webpack -p --progress --env prod",
        "css-paths": "node scripts/replace-css-image-paths.js",
        "postbuild": "rimraf build/* && cp -r dist/* build/ && cp ./static-server-files/* build/ && mkdir -p ../backend/wp-content/themes/redirect-theme/build/ && cp -r build/* ../backend/wp-content/themes/redirect-theme/build/"
    },
